#include <bits/stdc++.h>
using namespace std;

void solution()
{
    int n;
    cin >> n;
    double sum = 0.0;
    for (int i = 0; i < n; i++)
    {
        double x;
        cin >> x;
        sum += x;
    }
    double avg = sum / n;
    cout << fixed << setprecision(7) << avg << endl;
}

int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    solution();
    return 0;
}

