#include<bits/stdc++.h>
using namespace std;
void solution()
{
    int a,b,c;
    char s,q;
    cin>> a >> s >> b >>q >>c; 
    int ans;
    if(s == '+')
    {
        ans = a + b;
    }else if(s == '-'){
        ans = a - b;
    }else if(s == '*')
    {
        ans = a * b;
    }else{
        ans = a / b;
    }
    if(ans == c)
    {
        cout<< "Yes" <<endl;
    }else{
        cout<< ans <<endl;
    }       
}
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0); cout.tie(0);
    solution();
    return 0;
}
