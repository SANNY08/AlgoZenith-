#include <bits/stdc++.h>
using namespace std;

int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(0);
    cout.tie(0);
    char s;
    cin >> s;
    int n;
    cin >> n;
    int x[n];
    for (int i = 0; i < n; i++)
    {
        cin >> x[i];
    }
    for (int i = 0; i < n; i++)
    {
        for (int j = 0; j < x[i]; j++)
        {
            printf("%c",s);
        }
        printf("\n");
    }
    return 0;
}
